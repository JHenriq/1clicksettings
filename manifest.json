{
  "name": "1ClickSettings",
  "version": "1.01",
  "description": "Easy Accessible Settings",
  "permissions": ["management", "downloads", "downloads.open", "downloads.shelf", "history"],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "browser_action": {
    "default_title": "1ClickSettings",
    "default_icon": {
      "16": "images/Settings-icon16.png",
      "32": "images/Settings-icon32.png",
      "48": "images/Settings-icon48.png",
      "128": "images/Settings-icon128.png"
      },
    "default_popup": "popup.html"
  },
  
  "icons": {
    "16": "images/Settings-icon16.png",
    "32": "images/Settings-icon32.png",
    "48": "images/Settings-icon48.png",
    "128": "images/Settings-icon128.png"
  }
}
